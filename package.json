{
  "name": "package_tracker",
  "version": "1.0.0",
  "description": "A simple way demonstration of package tracking",
  "main": "dist/main.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist",
    "start:dev": "nodemon -e ts,json --exec ts-node src",
    "build": "yarn clean && tsc -p ./tsconfig.json",
    "watch": "tsc --watch -p ./tsconfig.json",
    "test": "jest --runInBand --detectOpenHandles --passWithNoTests --forceExit",
    "test:coverage": "yarn test --coverage",
    "lint:check": "npx xo",
    "lint:fix": "npx xo --fix --prettier"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "xo": "^0.58.0"
  },
  "_moduleAliases": {
    "@app": "./dist"
  }
}
